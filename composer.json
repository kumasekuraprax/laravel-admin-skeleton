{
    "name": "anluizmm/laravel-admin-skeleton",
    "description": "Estrutura completa para BackOffice, utilizando Laravel com AdminLTE + Controles em ACL + Docker",
    "keywords": ["zerodawn", "laravel-admin-skeleton "],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": "7.*",
        "fideloper/proxy": "^4.0",
        "laravel/framework": "5.6.*",
        "laravel/tinker": "^1.0",
        "laravelcollective/html": "^5.4.0",
        "jenssegers/date": "^3.3",
        "league/flysystem-aws-s3-v3": "^1.0",
        "predis/predis": "^1.1",
        "select2/select2": "^4.0",
        "unisharp/laravel-ckeditor": "^4.6",
        "yajra/laravel-datatables-oracle": "~8.0",
        "guzzlehttp/guzzle": "~6.0",
        "swiftmailer/swiftmailer": "6.0.2",
        "kodeine/laravel-acl": "~1.0@dev",
        "jeroennoten/laravel-adminlte": "^1.22",
        "santosalan/api-magic": "^0.0"
    },
    "require-dev": {
        "filp/whoops": "^2.0",
        "fzaninotto/faker": "^1.4",
        "mockery/mockery": "^1.0",
        "nunomaduro/collision": "^2.0",
        "phpunit/phpunit": "^7.0",
        "santosalan/laravel-crud": "^0.0"
    },
    "autoload": {
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
